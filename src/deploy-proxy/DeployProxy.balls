const NONCE_INCREASER

fn _INC_NONCE<z0>() -> (suc, one) {
    calldatacopy(z0, z0, calldatasize())
    one = 1
    suc = delegatecall(gas(), NONCE_INCREASER, z0, one, z0, z0)
}

fn _CREATE<z0>(one) {
    initcode_size = sub(calldatasize(), one)
    addr = create(callvalue(), 1, initcode_size)
    mstore(z0, addr)
    return(z0, 0x20)
}
